-- rect(x-4,y-4,#text*4+x+2,y+8,0)
 
  
-- text=format_text(text,x)
 print(scroll(t),x,y,12)
-- print(scroll(text),x,y,12)
 
end

cl=4
function format_text(text,x)
 
 w=128-x-4
 wt=flr(w/4)
 t="" 
 for i=0,#text,wt do
  t=t..sub(text,i+1,i+1+wt).."\n"
 end  
 return t
end

b_start_scroll=false
function start_scroll(text)
 b_start_scroll=true
 if time_out(dialog_timer) then
  set_timer(dialog_timer,#text+15)
 end
end

function scroll(text)
 if not b_start_scroll then
  start_scroll(text)
 end
 if dialog_timer.t > #text
 or time_out(dialog_timer) then
  i=#text
 else
  i=dialog_timer.t 
 end 
 return sub(text,1,i)
end
 
