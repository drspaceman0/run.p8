end 

--
-- states
function set_state(s)
 state=s   
 if state==st_intro then
  set_step(1,intro_steps) 
 end  
end

function upd_state()
 local s=state
 if(s==st_intro)intro()  
 if(s==st_main)main()
-- elseif s==st_gameover then gameover()
-- elseif s==st_win then win()
-- elseif s==st_peephole then peephole()
-- else error("invalid state:"..s)
-- end 
 upd_steps()
end
    
--
-- steps
step=0
step_w={}
function set_step(s,sw)
 step=s
 step_w=sw 
 set_timer(step_timer,step_w[step]*sec)
end

function upd_steps() 
 if time_out(step_timer) 
 and step<=count(step_w) then
  step+=1
  set_step(step,step_w)
 end
end

--
-- timer functions
function set_timer(t,w)
-- if not time_out(t) then
--  error("timer already set")
-- end
 t.t=0
 t.last=w 
end

function upd_timer(t)
 if not time_out(t) then 
	 if t.t >= t.last 
	 and t.last!=-1 then
	  t.t=0
	  t.last=0
	 end
	 t.t+=1
 end
end

function upd_all_timers()
 timers={g_timer,step_timer,dialog_timer}
 for t in all(timers) do
  upd_timer(t)
 end 
end
  
function time_out(t)
 return t.last==0
end
 
