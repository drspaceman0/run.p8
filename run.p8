function d_dialogue(text,x,y,c)
 
 --format text with newlines
 w=128-x
 wt=flr((w-8)/4)
 t="" 
 lines=0
 for i=0,#text,wt do
  t=t..sub(text,i+1,i+1+wt).."\n"
  lines+=1
 end 
 x1=x-1
 y1=y-1
 x2=w+x-4
 y2=y+(6*lines)
 rectfill(x1,y1,x2,y2,0)
