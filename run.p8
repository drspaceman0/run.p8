function show_footsteps(s)
 origin=s.o
 unit=128
 scale=s.s
 spd=s.p 
 
 local max_y=100
 clip(1,max_y,125,128-max_y-1)
  
 
 prev_y=0
 for x=0,128,8 do
  x1=(x+_t*spd)%128
  
  y1=(sin(x/unit)*cos(x/unit))*scale
  y1=y1+origin
  
  if y1>120 then
   y1=120
  end 
  
  print("\x98", x1,y1,8)
  prev_y=y1
--  for y2=y1,128,12 do
--   --print(_chars[-flr(rnd(19))%19], x1,y2,8)
--   print("\x96", x1,y2,8)
--  end 
 end 
end

function footstep(r)  
 for i=1,r do 
  y1=127-(i*4)
	 y2=y1+4 
	 for j=0,127,4 do
   x1=j
   x2=j+4
   b=4*(i)-flr(rnd(4)) 
   if b > 12 then
    b=17 
   end
   fillp(_fill_byte[b])
	  rectfill(x1,y1,x2,y2,8) 
	 end 
 end
end

